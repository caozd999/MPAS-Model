	nml_record name="AM_sedimentFluxIndex" mode="forward;analysis">
		<nml_option name="config_AM_sedimentFluxIndex_enable" type="logical" default_value=".false." units="unitless"
			description="If true, ocean analysis member sedimentFluxIndex is called."
      possible_values=".true. or .false."
		/>
		<nml_option name="config_AM_sedimentFluxIndex_compute_interval" type="character" default_value="output_interval" units="unitless"
			description="Timestamp determining how often analysis member computation should be performed."
			possible_values="Any valid time stamp, 'dt', or 'output_interval'"
		/>
		<nml_option name="config_AM_sedimentFluxIndex_output_stream" type="character" default_value="sedimentFluxIndexOutput" units="unitless"
			description="Name of the stream that the sedimentFluxIndex analysis member should be tied to."
			possible_values="Any existing stream name or 'none'"
		/>
		<nml_option name="config_AM_sedimentFluxIndex_compute_on_startup" type="logical" default_value=".true." units="unitless"
			description="Logical flag determining if an analysis member computation occurs on start-up."
			possible_values=".true. or .false."
		/>
		<nml_option name="config_AM_sedimentFluxIndex_write_on_startup" type="logical" default_value=".true." units="unitless"
			description="Logical flag determining if an analysis member write occurs on start-up."
			possible_values=".true. or .false."
		/>
		<nml_option name="config_AM_sedimentFluxIndex_use_lat_lon_coords" type="logical" default_value=".true." units="unitless"
			description="Logical flag determining if an analysis member run on global coordinate."
      possible_values=".true. or .false."
    />
	</nml_record>
	<packages>
		<package name="sedimentFluxIndexAMPKG" description="This package includes variables required for the sedimentFluxIndex analysis member."/>
	</packages>
	<var_struct name="sedimentFluxIndexAM" time_levs="1" packages="sedimentFluxIndexAMPKG">
    <var name="sedimentFluxIndexX" type="real" dimensions="nCells Time" units="m^3 s^{-3}"
			description="The index of sediment flux in x direction."
		/>
    <var name="sedimentFluxIndexY" type="real" dimensions="nCells Time" units="m^3 s^{-3}"
			description="The index of sediment flux in y direction."
		/>
	</var_struct>
	<streams>
		<stream name="sedimentFluxIndexOutput" type="output"
				mode="forward;analysis"
				filename_template="analysis_members/sedimentFluxIndex.$Y-$M-$D.nc"
				filename_interval="01-00-00_00:00:00"
				output_interval="00-00-00_00:01:00"
				reference_time="0001-01-01_00:00:00"
				packages="sedimentFluxIndexAMPKG"
				clobber_mode="truncate"
				runtime_format="single_file">
			<var name="xtime"/>
			<var name="sedimentFluxIndexX"/>
			<var name="sedimentFluxIndexY"/>
		</stream>
	</streams>
